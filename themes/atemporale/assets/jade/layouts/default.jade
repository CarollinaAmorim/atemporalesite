    body(itemtype="", itemscope)
        #fb-root
            script.
                (function (d, s, id) {
                    var js, fjs = d.getElementsByTagName(s)[0];
                    if (d.getElementById(id)) return;
                    js = d.createElement(s);
                    js.id = id;
                    js.src = "//connect.facebook.net/pt_BR/sdk.js#xfbml=1&version=v2.6&appId=1172179922816213";
                    fjs.parentNode.insertBefore(js, fjs);
                }(document, 'script', 'facebook-jssdk'));

        .site-loader
        +yield('eventuals')

        header.site-header
            .header-inner
                .container
                    .row
                        .header-table.col-md-12
                            .header-col.brand
                                a(href="{{ 'home'|page }}/#")
                                    +content('layout/navlogo.htm')
                            .header-col.main-nav
                                a.nav-toggle(href="#")
                                ul.nav
                                    li
                                        a(href!="{{ 'home'|page }}/#")
                                            !='Home'
                                    li
                                        a(href!="{{ 'home'|page }}/#about")
                                            !='Sobre nós'
                                    li
                                        a(href!="{{ 'home'|page }}/#services")
                                            !='O que fazemos?'
                                    li
                                        a(href!="{{ 'home'|page }}/#work")
                                            !='Nossos projetos'
                                    li
                                        a(href!="{{ 'home'|page }}/#contact")
                                            !='Contato'
                                    li
                                        a(href!="{{ 'blog'|page }}")
                                            ='Blog'
                                    li
                                        a(href="{{ 'account'|page }}")
                                            !='Area cliente'

                                    !='{% if user %}'
                                        li
                                            a(href="#", data-request="onLogout")
                                                !='Logout'
                                    !='{% endif %}'




        .site-wrapper
            .site-body
                +yield('pages')
                +component('googleTracker')





            footer.site-footer
                // inizio nascosto
                .footer-widgets
                    .container
                        .row
                            .col-md-5
                                +content('layout/footerdesc.htm')

                            .col-md-7
                                h5.widget-title.text-center
                                    !='Faça parte da nossa lista exclusiva'
                                .col-md-12.text-center.padding-bottom-20
                                    | Você receberá nossas melhores atualizações para você ficar sempre<br />por dentro do que acontece por aqui e no mundo do design.
                                +component('mailSignup')

                // fine nascosto

                .footer-bottom
                    .container
                        .row
                            .col-md-12
                                .footer-text
                                    +content('layout/footercc.htm')


                                .social-list
                                    ul
                                        li
                                            a(href="https://www.facebook.com/AtemporaleDesign/")
                                                span.fa.fa-facebook
                                        li
                                            a( href="https://twitter.com/Atemporale_D")
                                                span.fa.fa-twitter
                                        li
                                            a( href="https://www.instagram.com/atemporaledesign/")
                                                span.fa.fa-instagram
                                        li
                                            a( href="https://plus.google.com/+AtemporaleD/posts?hl=en")
                                                span.fa.fa-google-plus
                                        li
                                            a( href="https://github.com/AtemporaleNEXT")
                                                span.fa.fa-github


        +yield('scripts')
